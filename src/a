return (
    <div className="loginContainer">
      <Navbar sid={loggedInUser} />
      <h1>Log In</h1>
      <form onSubmit={handleSubmit} className="gridForm">
        <label className="leftColumn">User SID:</label>
        <input
          className="inputBox"
          type="text"
          placeholder="SID"
          value={sid}
          onChange={(e) => setSID(e.target.value)}
          required
        />
        <label className="leftColumn">Password:</label>
        <input
          className="inputBox"
          type="password"
          placeholder="Password"
          value={pass}
          onChange={(e) => setPass(e.target.value)}
          required
        />
        <label>
          <input type="checkbox" /> Remember me?
        </label>
        <a className="anchorLink" href="#">
          SSO Login
        </a>
        <input type="submit" className="submitButton" value="Log in" />
      </form>
    </div>
  );
};

export default LoginPage;
Navbar Component
Update the Navbar to receive and display the SID.

tsx
Copy code
import React from 'react';
import { FontAwesomeIcon } from '@fortawesome/react-fontawesome';
import { faUser } from '@fortawesome/free-solid-svg-icons';

interface NavbarProps {
  sid: string | null;
}

const Navbar: React.FC<NavbarProps> = ({ sid }) => {
  return (
    <div className="container">
      <div className="headingTitle">
        <h1 className="headingText">
          <img
            className="favicon"
            src="https://static.vecteezy.com/system/resources/previews/024/553/534/non_2x/lion-head-logo-mascot-wildlife-animal-illustration-generative-ai-png.png"
            alt="favicon"
          />
          Migration
        </h1>
        <div className="loginInfo">
          <FontAwesomeIcon icon={faUser} className="icon" />
          <span>{sid || 'SID'}</span>
        </div>
      </div>
      <div className="additionalInfo">
        <a href="https://engineers.ipmchase.net/docs/runtime-compute/app/" className="docs">
          Docs
        </a>
        <span>Help</span>
      </div>
    </div>
  );
};

export default Navbar;
